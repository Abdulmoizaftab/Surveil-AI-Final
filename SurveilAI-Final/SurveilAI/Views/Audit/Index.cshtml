    function fnExcelReport(id, name) {

        var tab_text = "<table border='2px'><tr bgcolor='#87AFC6'>";
        var textRange; var j = 0;
        tab = document.getElementById(id); // id of table

        console.log(tab);

        for (j = 0; j < tab.rows.length; j++) {
            tab_text = tab_text + tab.rows[j].innerHTML + "</tr>";
            //tab_text=tab_text+"</tr>";
        }

        tab_text = tab_text + "</table>";
        tab_text = tab_text.replace(/<A[^>]*>|<\/A>/g, "");//remove if u want links in your table
        tab_text = tab_text.replace(/<img[^>]*>/gi, ""); // remove if u want images in your table
        tab_text = tab_text.replace(/<input[^>]*>|<\/input>/gi, ""); // reomves input params

        var ua = window.navigator.userAgent;
        var msie = ua.indexOf("MSIE ");

        if (msie > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./))      // If Internet Explorer
        {
            txtArea1.document.open("txt/html", "replace");
            txtArea1.document.write(tab_text);
            txtArea1.document.close();
            txtArea1.focus();
            sa = txtArea1.document.execCommand("SaveAs", true, "DeviceFail.xls");
        }
        else                 //other browser not tested on IE 11
        {

            var str = encodeURIComponent(tab_text);
            var uri = 'data:text/csv;charset=utf-8,' + str;
            var downloadLink = document.createElement("a");
            downloadLink.href = uri;
            downloadLink.download = "ServerJournal-"+name+".xls";

            document.body.appendChild(downloadLink);
            downloadLink.click();
            document.body.removeChild(downloadLink);
        }
    }


    function Email()
    {
        $('#loading').show();
        var to = $("#emails").val();
        var subject = $("#eSubject").val();
        var message = $("#emessage").val();
        var array = [];
        var rows = $("#tbl tbody tr");
        var isAll = $("#tbl").attr("data-is-all");

        $.each(rows, function (index, row) {
            if (index > 0)
            {
                var columns = $(row).find("td");
                array[--index] = columns[0].innerHTML;
            }
        });

        var subcat = $(".tab-pane.active").find("table").attr("data-table");

        $.ajax({
            url: "@Url.Action("SendEmail", "Audit")",
            data: { to: to, mt_subject: subject, mailtext: message, cat: 3, table: array, isAll: isAll, subcat: subcat},
            type: "Post",
            dataType: "html",
            success: function (data) {
                $(".send_select").attr('disabled', 'disabled');
                $(".tab-pane.active").find("table tr.highlight").removeClass('highlight');
                if (data.indexOf("sent") != -1) {
                    $('#email-response').removeClass("text-danger");
                    $('#email-response').addClass("text-success");
                }
                else {
                    $('#email-response').removeClass("text-success");
                    $('#email-response').addClass("text-danger");
                }
                $('#email-response').text(data);
                $('#loading').hide();
            },
            error: function (data) {
                $(".send_select").attr('disabled', 'disabled');
                $(".tab-pane.active").find("table tr.highlight").removeClass('highlight');
                $('#loading').hide();
                alert("Error");
            }
        });
    }

    function getRows(all) {
        var rows;

        if (!all) {
            var array = [];

            array.push($(".tab-pane.active").find("table thead tr").clone());
            array.push($(".tab-pane.active").find("table tr.highlight").clone());

            var tbl = document.createElement('table');
            var tbdy = document.createElement('tbody');

            $(array).each(function () {
                tbdy.appendChild(this[0]);
            });

            tbl.appendChild(tbdy);
            rows = tbl;

        } else {
            rows = $(".tab-pane.active").find("table")[0];
        }

        return rows;
    }

    $(".tbody tr").click(function () {
        $(".tab-pane.active").find("table tbody tr").removeClass("highlight")
        var selected = $(this).hasClass("highlight");

        if (!selected) {
            $(this).addClass("highlight");
            $(".tab-pane.active").find("#send_select").removeAttr('disabled')
        }
    });

    $(".send_all").click(function () {
        displayRows(true);
    });

    $(".send_select").click(function () {
        displayRows(false);
    });

    function displayRows(all) {
        var table = getRows(all);
        $("#show_table").empty();
        $("#show_table").append("<table class='table table-striped table-sm' id='tbl' data-is-all='" + all + "'>" + table.innerHTML + "</table>");
    }

</script>


