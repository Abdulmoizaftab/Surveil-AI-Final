@model SurveilAI.Models.pvjournal
@{
    ViewBag.Title = "Audit";
    Layout = "~/Views/Shared/_Layout - Copy.cshtml";
}

<style>
    .highlight {
        background-color: #f0f6ff;
    }
</style>

<div class="content-wrapper">
    <div class="content container-fluid">
        <header class="page-header">
            <div class="mr-auto">
                <h1 class="separator">SURVEIL.AI</h1>
                <nav class="breadcrumb-wrapper" aria-label="breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="javascript:void(0)"><i class="icon dripicons-document-edit"></i></a></li>
                        <li class="breadcrumb-item"><a href="javascript:void(0)">Audit</a></li>
                        <li class="breadcrumb-item"><a href="javascript:void(0)">Server Journal</a></li>
                    </ol>
                </nav>
            </div>
        </header>

        <section class="page-content" id="myData">


            <div class="row">
                <div class="col-12">


                    <div class="col-md-12">
                        <div class="card">
                            <div class="row">
                                <div class="col-md-8">
                                    <h5 class="card-header">Server Journal</h5>
                                </div>
                                <div class="col-md-4">
                                    <label id="email-response" class="text-success" style="text-align: center; padding-top: 10px;"></label>
                                </div>
                            </div>
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-2">
                                        <div class="nav nav-tabs flex-column nav-pills" id="v-pills-tab" role="tablist" aria-orientation="vertical">
                                            <a class="nav-link active" id="v-pills-home-tab" data-bs-toggle="pill" href="#v-pills-home" role="tab" aria-controls="v-pills-home" aria-selected="true">All</a>
                                            <a class="nav-link" id="v-pills-role-tab" data-bs-toggle="pill" href="#v-pills-role" role="tab" aria-controls="v-pills-job" aria-selected="true">Rules</a>
                                            <a class="nav-link" id="v-pills-job-tab" data-bs-toggle="pill" href="#v-pills-jobs" role="tab" aria-controls="v-pills-job" aria-selected="true">Jobs</a>
                                        </div>
                                    </div>
                                    <div class="col-10">
                                        <div class="tab-content" id="v-pills-tabContent">
                                            <div class="tab-pane fadeIn show active" id="v-pills-home" role="tabpanel" aria-labelledby="v-pills-home-tab">
                                                <div class="row">
                                                    <div class="col-md-9" style="overflow: auto; max-height: 600px;">
                                                        <table id="bs4-table" class="table table-bordered" style="width: 100%" data-table="1">
                                                            <thead>
                                                                <tr>
                                                                    <th style="display: none;">ID</th>
                                                                    <th>Server Date Time</th>
                                                                    <th>Issuer Type</th>
                                                                    <th>PV User</th>
                                                                    <th>Device ID</th>
                                                                    <th>Command</th>
                                                                    <th>Var Data</th>
                                                                    <th>Error Code</th>
                                                                    <th>Desktop User</th>
                                                                </tr>
                                                            </thead>

                                                            <tbody id="bs4-tbody" class="tbody">
                                                                @if (ViewBag.pvjournal.Count == 0)
                                                                {

                                                                }
                                                                else
                                                                {
                                                                    foreach (var item in ViewBag.pvjournal)
                                                                    {
                                                                        <tr>
                                                                            <td style="display: none;">@item[0].ToString()</td>
                                                                            <td>@item[1].ToString()</td>
                                                                            <td>@item[2].ToString()</td>
                                                                            <td>@item[3].ToString()</td>
                                                                            <td>@item[4].ToString()</td>
                                                                            <td>@item[5].ToString()</td>
                                                                            <td>@item[6].ToString()</td>
                                                                            <td>@item[7].ToString()</td>
                                                                            <td>@item[8].ToString()</td>
                                                                        </tr>
                                                                    }
                                                                }
                                                            </tbody>
                                                        </table>

                                                    </div>
                                                    <div class="col-md-3">
                                                        <button style="margin-bottom:5px" class="btn btn-primary d-block col-12" onclick="window.location.reload();">Refresh</button>
                                                        @*<button style="margin-bottom:5px" class="btn btn-primary d-block col-12" onclick="">Previous</button>
                                                            <button style="margin-bottom:5px" class="btn btn-primary d-block col-12" onclick="">Next</button>*@
                                                        <a class="btn btn-primary d-block col-12" onclick="fnExcelReport('bs4-table','All');" style="float: right;color: white;margin-bottom:5px;">Export<iframe id="txtArea1" style="display:none"></iframe></a>
                                                        <button id="send_select" disabled style="margin-bottom:5px" class="btn btn-primary d-block col-12 send_select" data-bs-toggle="modal" data-bs-target="#Semail">Send</button>
                                                        <button id="send_all" style="margin-bottom:5px" class="btn btn-primary d-block col-12 send_all" data-bs-toggle="modal" data-bs-target="#Semail">Send All</button>
                                                        <button style="margin-bottom:5px" class="btn btn-sm btn-warning d-block col-12" data-bs-toggle="modal" data-bs-target="#Acts">Filter</button>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="tab-pane fadeIn" id="v-pills-role" role="tabpanel" aria-labelledby="v-pills-role-tab">
                                                <div class="row">
                                                    <div class="col-md-9" style="overflow: auto; max-height: 600px;">
                                                        <table id="bs-role-table" class="table table-bordered" style="width: 100%" data-table="4">
                                                            <thead>
                                                                <tr>
                                                                    <th style="display: none;">ID</th>
                                                                    <th>Server Date Time</th>
                                                                    <th>Issuer Type</th>
                                                                    <th>PV User</th>
                                                                    <th>Device ID</th>
                                                                    <th>Command</th>
                                                                    <th>Var Data</th>
                                                                    <th>Error Code</th>
                                                                    <th>Desktop User</th>
                                                                </tr>
                                                            </thead>

                                                            <tbody class="tbody">
                                                                @foreach (var item in ViewBag.pvRole)
                                                                {
                                                                    <tr>
                                                                        <td style="display: none;">@item[0].ToString()</td>
                                                                        <td>@item[1].ToString()</td>
                                                                        <td>@item[2].ToString()</td>
                                                                        <td>@item[3].ToString()</td>
                                                                        <td>@item[4].ToString()</td>
                                                                        <td>@item[5].ToString()</td>
                                                                        <td>@item[6].ToString()</td>
                                                                        <td>@item[7].ToString()</td>
                                                                        <td>@item[8].ToString()</td>
                                                                    </tr>
                                                                }
                                                            </tbody>
                                                        </table>

                                                    </div>
                                                    <div class="col-md-3">
                                                        <button style="margin-bottom:5px" class="btn btn-primary d-block col-12" onclick="window.location.reload();">Refresh</button>
                                                        @*<button style="margin-bottom:5px" class="btn btn-primary d-block col-12" onclick="">Previous</button>
                                                            <button style="margin-bottom:5px" class="btn btn-primary d-block col-12" onclick="">Next</button>*@
                                                        <a class="btn btn-primary d-block col-12" onclick="fnExcelReport('bs-role-table','Role');" style="float: right;color: white;margin-bottom:5px;">Export<iframe id="txtArea1" style="display:none"></iframe></a>
                                                        <button id="send_select" disabled style="margin-bottom:5px" class="btn btn-primary d-block col-12 send_select" data-bs-toggle="modal" data-bs-target="#Semail">Send</button>
                                                        <button id="send_all" style="margin-bottom:5px" class="btn btn-primary d-block col-12 send_all" data-bs-toggle="modal" data-bs-target="#Semail">Send All</button>
                                                        <button style="margin-bottom:5px" class="btn btn-sm btn-warning d-block col-12" data-bs-toggle="modal" data-bs-target="#Acts">Filter</button>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="tab-pane fadeIn" id="v-pills-jobs" role="tabpanel" aria-labelledby="v-pills-job-tab">
                                                <div class="row">
                                                    <div class="col-md-9" style="overflow: auto; max-height: 600px;">
                                                        <table id="bs-job-table" class="table table-bordered" style="width: 100%" data-table="3">
                                                            <thead>
                                                                <tr>
                                                                    <th style="display: none;">ID</th>
                                                                    <th>Server Date Time</th>
                                                                    <th>Issuer Type</th>
                                                                    <th>PV User</th>
                                                                    <th>Device ID</th>
                                                                    <th>Command</th>
                                                                    <th>Var Data</th>
                                                                    <th>Error Code</th>
                                                                    <th>Desktop User</th>
                                                                </tr>
                                                            </thead>

                                                            <tbody class="tbody">
                                                                @foreach (var item in ViewBag.pvJob)
                                                                {
                                                                    <tr>
                                                                        <td style="display: none;">@item[0].ToString()</td>
                                                                        <td>@item[1].ToString()</td>
                                                                        <td>@item[2].ToString()</td>
                                                                        <td>@item[3].ToString()</td>
                                                                        <td>@item[4].ToString()</td>
                                                                        <td>@item[5].ToString()</td>
                                                                        <td>@item[6].ToString()</td>
                                                                        <td>@item[7].ToString()</td>
                                                                        <td>@item[8].ToString()</td>
                                                                    </tr>
                                                                }
                                                            </tbody>
                                                        </table>

                                                    </div>
                                                    <div class="col-md-3">
                                                        <button style="margin-bottom:5px" class="btn btn-primary d-block col-12" onclick="window.location.reload();">Refresh</button>
                                                        @*<button style="margin-bottom:5px" class="btn btn-primary d-block col-12" onclick="">Previous</button>
                                                            <button style="margin-bottom:5px" class="btn btn-primary d-block col-12" onclick="">Next</button>*@
                                                        <a class="btn btn-primary d-block col-12" id="btnExport" onclick="fnExcelReport('bs-job-table','Job');" style="float: right;color: white;margin-bottom:5px;">Export<iframe id="txtArea1" style="display:none"></iframe></a>
                                                        <button id="send_select" disabled style="margin-bottom:5px" class="btn btn-primary d-block col-12 send_select" data-bs-toggle="modal" data-bs-target="#Semail">Send</button>
                                                        <button id="send_all" style="margin-bottom:5px" class="btn btn-primary d-block col-12 send_all" data-bs-toggle="modal" data-bs-target="#Semail">Send All</button>
                                                        <button style="margin-bottom:5px" class="btn btn-sm btn-warning d-block col-12" data-bs-toggle="modal" data-bs-target="#Acts">Filter</button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
</div>

<div class="modal fade" id="Acts" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" style="display: none;" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document" style="min-width:900px;">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLongTitle">Server Journal Filter</h5>
            </div>
            <div class="modal-body mCustomScrollbar _mCS_5 mCS-autoHide mCS_no_scrollbar" style="overflow: visible;">
                <div id="mCSB_5" class="mCustomScrollBox mCS-minimal-dark mCSB_vertical mCSB_outside" tabindex="0" style="max-height: none;">
                    <div id="mCSB_5_container" class="mCSB_container mCS_y_hidden mCS_no_scrollbar_y" style="position:relative; top:0; left:0;" dir="ltr">

                        <div class="card-body">
                            <div class="mb-3 row container-fluid">
                                <div class="col-md-12">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div style="min-height:350px;" class="AllDiv2 eventR">
                                                <table class="table table-bordered">
                                                    <tbody>
                                                        <tr>
                                                            <td>Period:</td>
                                                            <td>
                                                                <div class="row">
                                                                    <div class="col-md-6">
                                                                        From
                                                                        <div class="input-group date dp-years">
                                                                            <input class="form-control" id="DateFrom" name="DateFrom" placeholder="mm/dd/yyyy" type="text" value="">
                                                                            <span class="input-group-addon action">
                                                                                <i class="icon dripicons-calendar"></i>
                                                                            </span>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col-md-6">
                                                                        To
                                                                        <div class="input-group date dp-years">
                                                                            <input class="form-control" id="DateTo" name="DateFrom" placeholder="mm/dd/yyyy" type="text" value="">
                                                                            <span class="input-group-addon action">
                                                                                <i class="icon dripicons-calendar"></i>
                                                                            </span>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="row">
                                                                    <div class="col-md-12">
                                                                        <small class="text-danger" id="date-valid-new" style="display:none;">Start date should be less than End date</small>
                                                                    </div>
                                                                </div>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td>Resource</td>
                                                            <td><input id="res" type="text" class="form-control BlockSpecialSS" tyle="float:right;" /></td>
                                                        </tr>
                                                        <tr>
                                                            <td>Operation Type</td>
                                                            <td>
                                                                <select id="oprtyp" class="form-select select-c" style="float:right;">
                                                                    <option></option>
                                                                    @foreach (var item in ViewBag.pvOperationType)
                                                                    {
                                                                        <option id=@item>@item</option>
                                                                    }
                                                                </select>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td>Commands</td>
                                                            <td>
                                                                <div class="input-group" style="float: left;">
                                                                    <input id="cmd" type="text" class="form-control BlockSpecialSS" tyle="float:right;" />
                                                                    <span class="input-group-addon action" data-bs-toggle="modal" data-bs-target="#Commands">
                                                                        <i class="icon dripicons-search"></i>
                                                                    </span>
                                                                </div>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td>Action</td>
                                                            <td>
                                                                <select id="act" class="form-select select-c" style="float:right;">
                                                                    <option id="All">All</option>
                                                                    @foreach (var item in ViewBag.pvActions)
                                                                    {
                                                                        <option id=@item>@item</option>
                                                                    }
                                                                </select>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td>User</td>
                                                            <td><input id="user" type="text" class="form-control BlockSpecialSS" tyle="float:right;" /></td>
                                                        </tr>
                                                        <tr>
                                                            <td>Result</td>
                                                            <td><input id="result" type="text" class="form-control BlockSpecialSS" tyle="float:right;" /></td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button id="apply" type="button" class="btn btn-secondary btn-outline" data-bs-dismiss="modal">Apply</button>
                <button id="clear" type="button" class="btn btn-secondary btn-outline">Clear</button>
                <button type="button" class="btn btn-secondary btn-outline" data-bs-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>

