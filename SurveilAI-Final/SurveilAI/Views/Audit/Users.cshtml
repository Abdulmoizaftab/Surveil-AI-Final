                                    <button id="send_all" style="margin-bottom:5px" class="btn btn-primary d-block col-12" data-bs-toggle="modal" data-bs-target="#Semail">Send All</button>
                                    <button style="margin-bottom:5px" class="btn btn-sm btn-warning d-block col-12" data-bs-toggle="modal" data-bs-target="#Acts">Filter</button>
