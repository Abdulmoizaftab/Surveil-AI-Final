<script>


    $('.restrictSpecial').bind('paste', function () {
        var elmId = $(this).attr('id');
        CheckSpecialChar(elmId);
    });
    function CheckSpecialChar(id) {
        setTimeout(function () {
            var elmId = $('#' + id);
            //get the value of the input text
            var data = elmId.val();
            //replace the special characters to ''
            var dataFull = data.replace(/[^\w\s-.]/gi, '');
            //set the new value of the input text without special characters
            elmId.val(dataFull);
        });
    }

    $('.restrictSpecial').on('keypress', function (event) {
        var regex = new RegExp("^[a-zA-Z0-9-.\\-\\s]+$");
        var key = String.fromCharCode(!event.charCode ? event.which : event.charCode);
        if (!regex.test(key)) {
            event.preventDefault();
            return false;
        }
    });



    function isIpAddress(s) {
      if (s == "") { return true;}
      else if (typeof s !== 'string') { return false; }
      // There must be 4 parts separated by dots.
      var parts = s.split('.');
      if (parts.length !== 4) { return false; }
      // Each of the four parts must be an integer in the range 0 to 255.
      for (var i = 0; i < 4; ++i) {
        var part = parts[i];
        // Each part must consist of 1 to 3 decimal digits.
        if (!/^\d{1,3}$/.test(part)) { return false; }
        var n = +part;
        if (0 > n || n > 0xff) { return false; }
      }
      return true;
    }
    var pattern = /\b(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\b/;
    x = 46;
    $('.IpValid').keypress(function (e) {
        if (e.which != 8 && e.which != 0 && e.which != x && (e.which < 48 || e.which > 57)) {
            console.log(e.which);
            return false;
        }
    }).keyup(function () {
        var this1 = $(this);
        var valId = $(this).data("val")
        var aa = "#" + valId;
        if (this1.val() == "") {
            $(""+aa+"").hide();
        }
        else if (!pattern.test(this1.val())) {

            $(""+aa+"").show();
            while (this1.val().indexOf("..") !== -1) {
                this1.val(this1.val().replace('..', '.'));
            }
            x = 46;
        } else {
            x = 0;
            var lastChar = this1.val().substr(this1.val().length - 1);
            if (lastChar == '.') {
                this1.val(this1.val().slice(0, -1));
            }
            var ip = this1.val().split('.');
            if (ip.length == 4) {

            }
            $(""+aa+"").hide();
        }
    });



     $('.selected-comm').click(function () {
            var commands = $('#cmd').val();
            var commandTxt = $(this).text();
            var commNo = $(this).children().eq(1).val();
            if (commands == "") {
                $('#cmd').val(commandTxt);
                $('#cmd-no').val(commNo);
            }
            else {
                $('#cmd').val(commandTxt);
                $('#cmd-no').val(commNo);
            }

            $('#Commands').modal('toggle');
        });

    $("#apply").on('click',
        function (e) {
            debugger;
            e.preventDefault();
            var datefrom = $("#DateFrom").val();
            var dateto = $("#DateTo").val();

            var startDate = new Date($('#DateFrom').val());
            var endDate = new Date($('#DateTo').val());

            //if (startDate == "Invalid Date" || endDate == "Invalid Date")
            //{
            //     $('#date-valid-edit').hide();
            //     $('#date-valid-nedit').show();
            //     return false;
            // }


            if (startDate > endDate) {
                $('#date-valid-nedit').hide();
                $('#date-valid-edit').show();
                return false;

            }
            $('#date-valid-nedit').hide();
            $('#date-valid-edit').hide();




            var oprtyp = $("#oprtyp").val();
            if (oprtyp == "Select Status") {
                oprtyp = "";
            }
            var device = $("#res").val();
            //var cmd = parseInt($("#cmd").val());
            var cmd = "";
            //var action = $("#act").val();
            var action = $("#cmd").val();
            if (!isIpAddress(action)) {
                return false;
            }

            var user = $("#user").val();
            if (!isIpAddress(user)) {
                return false;
            }
            var result = $("#result").val();

            $.ajax({
            url: "@Url.Action("ApplyFilter", "Audit")",
                data: {cat:1, lastLoginDateFrom: datefrom, lastLoginDateTo: dateto, operationType: oprtyp, deviceId: device, commands:cmd, action:action, user:user, result:result },
            type: "Post",
            dataType: "html",
                success: function (data) {
                    window.location = "@Url.Action("Users", "Audit")";
            },
            error: function (data) {
                alert("Error");
            }
        });
    });

    $("#clear").on('click',
        function() {
            $("#DateFrom").val('');
            $("#DateTo").val('');
            $("#res").val('');
            $("#cmd").val('');
            $("#act").val('');
            $("#user").val('');
            $("#result").val('');
        });

    function Email() {
        $('#loading').show();
        var to = $("#emails").val();
        var subject = $("#eSubject").val();
        var message = $("#emessage").val();
        var array = [];
        var rows = $("#tbl tbody tr");
        var isAll = $("#tbl").attr("data-is-all");

        $.each(rows,
            function(index, row) {
                if (index > 0) {
                    var columns = $(row).find("td");
                    array[--index] = columns[0].innerHTML;
                }
            });
        debugger;
        $.ajax({
            url: "@Url.Action("SendEmail", "Audit")",
            data: { to: to, mt_subject: subject, mailtext: message, cat: 1, table: array, isAll: isAll },
            type: "Post",
            dataType: "html",
            success: function(data) {
                $("#send_select").attr('disabled', 'disabled');
                $('#bs4-table tr.highlight').removeClass('highlight');
                if (data.indexOf("sent") != -1) {
                    $('#email-response').removeClass("text-danger");
                    $('#email-response').addClass("text-success");
                }
                else {
                    $('#email-response').removeClass("text-success");
                    $('#email-response').addClass("text-danger");
                }
                $('#email-response').text(data);
                $('#loading').hide();
            },
            error: function(data) {
                $("#send_select").attr('disabled', 'disabled');
                $('#bs4-table tr.highlight').removeClass('highlight');
                $('#loading').hide();
                alert("Error");
            }
        });
    }

    function getRows(all) {
        var rows;

        if (!all) {
            var array = [];

            array.push($('#bs4-table thead tr').clone());
            array.push($('#bs4-table tr.highlight').clone());

            var tbl = document.createElement('table');
            var tbdy = document.createElement('tbody');

            $(array).each(function() {
                tbdy.appendChild(this[0]);
            });

            tbl.appendChild(tbdy);
            rows = tbl;

        } else {
            rows = $('#bs4-table')[0];
        }

        return rows;
    }

    function fnExcelReport() {

        var tab_text = "<table border='2px'><tr bgcolor='#87AFC6'>";
        var textRange;
        var j = 0;
        tab = document.getElementById('bs4-table'); // id of table

        for (j = 0; j < tab.rows.length; j++) {
            tab_text = tab_text + tab.rows[j].innerHTML + "</tr>";
        }

        tab_text = tab_text + "</table>";
        tab_text = tab_text.replace(/<A[^>]*>|<\/A>/g, ""); //remove if u want links in your table
        tab_text = tab_text.replace(/<img[^>]*>/gi, ""); // remove if u want images in your table
        tab_text = tab_text.replace(/<input[^>]*>|<\/input>/gi, ""); // reomves input params

        var ua = window.navigator.userAgent;
        var msie = ua.indexOf("MSIE ");

        if (msie > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./)) // If Internet Explorer
        {
            txtArea1.document.open("txt/html", "replace");
            txtArea1.document.write(tab_text);
            txtArea1.document.close();
            txtArea1.focus();
            sa = txtArea1.document.execCommand("SaveAs", true, "DeviceFail.xls");
        } else //other browser not tested on IE 11
        {
            var str = encodeURIComponent(tab_text);
            var uri = 'data:text/csv;charset=utf-8,' + str;
            var downloadLink = document.createElement("a");
            downloadLink.href = uri;
            downloadLink.download = "Users.xls";

            document.body.appendChild(downloadLink);
            downloadLink.click();
            document.body.removeChild(downloadLink);
        }
    }

    $('#bs4-tbody tr').click(function () {
        debugger;

        $("#bs4-tbody tr").removeClass("highlight");
        var selected = $(this).hasClass("highlight");

        if (selected==false) {
            $(this).addClass("highlight");
            $("#send_select").removeAttr('disabled');
        }

    });

    $("#send_all").click(function() {
        displayRows(true);
    });

    $("#send_select").click(function() {
        displayRows(false);
    });

    $('#bs-command-table tr').click(function() {
        $("#bs-command-table tr").removeClass("highlight");
        var selected = $(this).hasClass("highlight");

        if (!selected) {
            $(this).addClass("highlight");
            $("#cmd_select").removeAttr('disabled');
        }
    });

    function displayRows(all) {
        var table = getRows(all);
        $("#show_table").empty();
        $("#show_table").append("<table id='tbl' data-is-all='" + all + "'>" + table.innerHTML + "</table>");
    }

    function ApplyCommand() {
        var code = $("#bs-command-table tr.highlight td")[0].innerHTML;
        $("#cmd").val(code);
    }
</script>


